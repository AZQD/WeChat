/* pages/elipsis/elipsis.wxss */
.warp{
    display: flex;
    -webkit-display: flex;
    justify-content: center;
    align-items: center;
}
.item{
    flex-grow: 1;
    border: 1rpx solid red;
    display: flex;
    justify-content: center;
}
.left{
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 200rpx;
    text-align: right;
}
.right{
    width: 28rpx;
    height: 28rpx;
    display: inline-block;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
}
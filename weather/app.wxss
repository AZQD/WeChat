page {
    font: 13px "微软雅黑";
    background-image: url("data:image/jpg;base64,");
    background-repeat: no-repeat;
    background-size: 100% 100%;
    font-weight: bold;
    line-height: 1.5;
    color: white;
}

/*外部滚动容器的样式*/
.outer{
    position: absolute;
    top: 0;
    bottom: 0;
}

.outer .inner{
    margin: 10px;
}

/*基本信息的样式*/
.base_info{
    display: flex;
    flex-direction: column;
    text-align: right;
}

/*当前信息*/
.wt_info{
    font-size: 40px;
}

/*天气描述信息*/
.wt_info .desc{
    font-size: 25px;
    margin-left: 20px;
}

/*气象指数*/
.wt_index{
    background-color: rgba(0, 0, 0, 0.5);
    padding: 10px;
    display: flex;
    flex-direction: column;
}

.wt_index text{
    margin-bottom: 10px;
}

.shade-wrap{
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    z-index: 999;
    background-color: rgba(0, 0, 0, 0.5);
    display:flex;
    align-items:center;
    justify-content: center;
}
.shade-box{
    width: 360rpx;
    height: 360rpx;
    background-color: pink;
    display: flex;
    align-items: center;
    justify-content: center;
}
.box{
    height: 1000rpx;
}
